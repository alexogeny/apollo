    "@types/node": "^20.11.30",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "bun-types": "^1.0.12",
    "happy-dom": "^14.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.1.8",
    "typescript": "^5.4.5",
    "vite": "^5.1.6"
  },
  "packageManager": "bun@1.0.29",
